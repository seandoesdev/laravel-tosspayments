{
    "name": "seandoesdev/tosspayments",
    "description": "Laravel package for Toss Payments Widget integration",
    "type": "library",
    "keywords": [
        "laravel",
        "toss",
        "payments",
        "widget",
        "korea"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "seandoesdev",
            "email": "seandoesdev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Seandoesdev\\TossPayments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seandoesdev\\TossPayments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seandoesdev\\TossPayments\\TossPaymentsServiceProvider"
            ],
            "aliases": {
                "TossPayments": "Seandoesdev\\TossPayments\\Facades\\TossPayments"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}